import java.util.Scanner;

public class CenterDigit {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String num = sc.next();  
        sc.close();

        int len = num.length();

        if (len % 2 == 0) {
            System.out.println(-1);
        } else {
            System.out.println(num.charAt(len / 2));
        }
    }
}
